  START
  INPUT num1,num2,num3
  IF num1>num2 AND num1>num3 THEN
  PRINT num1 is greater
  ELSE IF num2>num1 AND num2>num3 THEN
  PRINT num2 is greater
  ELSE
  PRINT num3 is greater
  END IF
  STOP
  
  
Question 3.
#include<iostream>
using namespace std;
int checkEven(int);
int checkEven(int a){
     int i;
     i = a%2==0;
     return i;
}

int main()
{
    bool even;
    int num;
    cout<<"Input num";
    cin>>num;
    even = checkEven(num);
    
    if (even == true){
    cout<< "Even Number";
    }
    else
    cout<<"Odd Number";
}

